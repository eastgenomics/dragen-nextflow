params {
    ref              = 's3://joseph-dragen-test/prebuiltDragenReference/'
    fastq1           = 's3://joseph-dragen-test/dragenTestInputs/NA24385-AJ-Son-R1-NS_S33_L001_R2_001.fastq.gz'
    fastq2           = 's3://joseph-dragen-test/dragenTestInputs/NA24385-AJ-Son-R1-NS_S33_L001_R1_001.fastq.gz'
    rgid             = 'NA24385_RGID'
    rgsm             = 'NA24385'
    prefix           = 'NA24385'
    output_dir       = 'results'
    intermediate_dir = 'tmp'
    dragen_container = "public.ecr.aws/docker/library/oraclelinux:8"
    lic              = ""
}

process {
  cpus = 20
  memory = 200.GB
  container = "${params.dragen_container}"
  containerOptions = '-v /opt/edico/bin:/opt/edico/bin'
}

aws {
    batch {
        volumes = '/opt/edico/bin:/opt/edico/bin'
    }
}